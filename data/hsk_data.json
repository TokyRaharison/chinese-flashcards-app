    "sentences": [
      {
        "character": "您在做什么？",
        "pinyin": "Nín zài zuò shénme?",
        "translation": "Que faites-vous ?"
      },
      {
        "character": "我非常高兴。",
        "pinyin": "Wǒ fēicháng gāoxìng.",
        "translation": "Je suis très content."
      },
      {
        "character": "我们一起学习吧。",
        "pinyin": "Wǒmen yīqǐ xuéxí ba.",
        "translation": "Étudions ensemble."
      },
      {
        "character": "他已经回家了。",
        "pinyin": "Tā yǐjīng huí jiā le.",
        "translation": "Il est déjà rentré à la maison."
      },
      {
        "character": "昨天天气很好。",
        "pinyin": "Zuótiān tiānqì hěn hǎo.",
        "translation": "Le temps était très beau hier."
      },
      {
        "character": "明天我要去公司。",
        "pinyin": "Míngtiān wǒ yào qù gōngsī.",
        "translation": "Demain je veux aller à l'entreprise."
      },
      {
        "character": "你会说汉语吗？",
        "pinyin": "Nǐ huì shuō Hànyǔ ma?",
        "translation": "Sais-tu parler chinois ?"
      },
      {
        "character": "我喜欢吃中国菜。",
        "pinyin": "Wǒ xǐhuan chī Zhōngguó cài.",
        "translation": "J'aime manger la cuisine chinoise."
      },
      {
        "character": "他在房间里看书。",
        "pinyin": "Tā zài fángjiān lǐ kàn shū.",
        "translation": "Il lit un livre dans la chambre."
      },
      {
        "character": "我们去火车站吧。",
        "pinyin": "Wǒmen qù huǒchē zhàn ba.",
        "translation": "Allons à la gare."
      },
      {
        "character": "这个问题很难。",
        "pinyin": "Zhège wèntí hěn nán.",
        "translation": "Cette question est très difficile."
      },
      {
        "character": "我想喝一杯咖啡。",
        "pinyin": "Wǒ xiǎng hē yī bēi kāfēi.",
        "translation": "Je veux boire un café."
      },
      {
        "character": "他每天跑步。",
        "pinyin": "Tā měitiān pǎobù.",
        "translation": "Il court tous les jours."
      },
      {
        "character": "我的朋友喜欢旅游。",
        "pinyin": "Wǒ de péngyǒu xǐhuan lǚyóu.",
        "translation": "Mon ami aime voyager."
      },
      {
        "character": "今天我很忙。",
        "pinyin": "Jīntiān wǒ hěn máng.",
        "translation": "Aujourd'hui je suis très occupé."
      },
      {
        "character": "她的衣服很漂亮。",
        "pinyin": "Tā de yīfu hěn piàoliang.",
        "translation": "Ses vêtements sont très beaux."
      },
      {
        "character": "我不知道他的电话。",
        "pinyin": "Wǒ bù zhīdao tā de diànhuà.",
        "translation": "Je ne sais pas son numéro de téléphone."
      },
      {
        "character": "我们可以一起吃饭吗？",
        "pinyin": "Wǒmen kěyǐ yīqǐ chī fàn ma?",
        "translation": "Pouvons-nous manger ensemble ?"
      },
      {
        "character": "他正在准备考试。",
